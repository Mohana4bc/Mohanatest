<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="com.axium.Axium.controller.BinsAvailable"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page>
				<customHeader>
					<Bar>
						<contentLeft>
							<Button id="backBins" type="Emphasized" text="BACK" press="onPressBack"/>
						</contentLeft>
						<contentMiddle>
							<Text text="Storage Bins"></Text>
						</contentMiddle>
					</Bar>
				</customHeader>
				<content>
					<List id="BinsAvailable" items="{BinsAvailable>/binsAvailable}" headerText="Bins Available" mode="SingleSelectMaster">
						<StandardListItem title="{BinsAvailable>StorageBin}"/>
					</List>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>