<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="com.axium.Axium.controller.PutAway"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page>
				<customHeader>
					<Bar>
						<contentLeft>
							<Button id="back" type="Emphasized" text="BACK" press="onPressBack"/>
						</contentLeft>
						<contentMiddle>
							<Text text="RM Put Away"></Text>
						</contentMiddle>
					</Bar>
				</customHeader>
				<content>
					<SearchField id="id1" showSearchButton="false" placeholder="Scan Purchase order" maxLength="10"/>
					<!--<Button icon="sap-icon://bar-code" text="Scan" tooltip="Scan barcode" visible="true" press="onScanForValue"/>-->
				</content>
				<footer>
					<Bar>
						<contentRight>
							<Button text="Next" press="onNext" type="Accept"/>
						</contentRight>
					</Bar>
				</footer>
			</Page>
		</pages>
	</App>
</mvc:View>