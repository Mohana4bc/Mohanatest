<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="com.axium.Axium.controller.AvailableBinReturn"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page>
				<customHeader>
					<Bar>
						<contentLeft>
							<Button text="Back" press="onPressBack" type="Emphasized"></Button>
						</contentLeft>
						<contentMiddle>
							<Text text="Storage Bins"></Text>
						</contentMiddle>
					</Bar>
				</customHeader>
				<content>
					<List id="AvailableBinReturn" items="{AvailableBinReturn>/binsAvailable}" headerText="Bins Available" mode="SingleSelectMaster">
						<StandardListItem title="{AvailableBinReturn>StorageBin}"/>
					</List>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>