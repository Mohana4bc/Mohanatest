							iconFirst="true" type="Emphasized" press="onPressFGPick" icon="sap-icon://bar-code"/>
						<Button id="idPI" text="Physical Inventory" class="sapMBtnContent" iconDensityAware="false" visible="false" enabled="true" width="325px"
							iconFirst="true" type="Emphasized" press="onPhysicalInventory" icon="sap-icon://bar-code"/>
						<Button id="idSTA" text="Stock Transfer App" class="sapMBtnContent" iconDensityAware="false" visible="false" enabled="true" width="325px"
							iconFirst="true" type="Emphasized" press="onInventoryPress" icon="sap-icon://bar-code"/>
						<!--<Button text="Bin To Bin Transfer" iconDensityAware="false" visible="true" enabled="true" width="219px" iconFirst="true" type="Emphasized"-->
