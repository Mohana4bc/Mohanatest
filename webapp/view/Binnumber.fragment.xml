<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
	<Dialog afterClose="onDialogClose" title="Scan Bin Number to Save">
		<content>
			<Input id="fgPutAwayBinId" enabled="true" placeholder="Please Scan Bin Number" change="onBinScan"></Input>
			<Bar>
				<contentRight>
					<Button text="Save" id="idSubmit" type="Accept" icon="sap-icon://save" press="onSave"/>
				</contentRight>
			</Bar>
		</content>
	</Dialog>
</core:FragmentDefinition>